<h2>Book info</h2>
<h3>Editing Book</h3>
<%= form_with model: @book do |f| %>
  <h4>Image</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Update Book' %>
<% end %>
<%= link_to "Back", books_path %>
<%= link_to "Show", book_path(@book) %>